.header{
  display: flex;
  width: 100%;
  justify-content: flex-end;
  height: 60px;
  align-items: center;
}
.timer{
  height: 24px;
  line-height: 24px;
  padding:0px 10px;
  border-radius: 12px;
  margin-right: 12px;
  font-size: 14px;
  background-color: #f0f0f0;
}
.btn{
  background-color: #f0f0f0;
  border-radius: 0px;
  margin-top: 50%;
  color: #5a5a5a;
  font-size: 16px;
  width: 180px;
  border: none;
  box-shadow: 1px 1px 4px #cdcdcd;
}
.btn ::after{
  border: none;
}
