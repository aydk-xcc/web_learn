/**index.wxss**/
page{
  display: flex;
  justify-content: flex-start;
  background-color: #f0f0f0;
}
.item{
  display: flex;
  width: 100%;
  height: 50px;
  background-color: white;
  flex-direction: row;
  line-height: 50px;
  margin-bottom: 8px;
  padding:0px 8px;
}
.start_note{
  padding: 4px 12px;
  background-color: white;
  color: #8a8a8a;
  box-shadow: 2px 2px 4px #cdcdcd;
  margin-top: 80%;
}
.model{
  display: flex;
  flex-direction: column;
  position: absolute;
  top: 30%;
  width: 80%;
  background-color: white;
  padding: 20px;
  border-radius: 4px;
}
.model_title{
  display: flex;
  justify-content: center;
  margin-bottom: 8px;
}
.cancle{
  display: flex;
  justify-content: center;
  flex-grow: 1.0;
  padding: 8px 8px;
  margin-right: 10px;
  background-color: #f0f0f0;
  color: #8a8a8a;
  border-radius: 4px;
}
.cancle :hover{
  opacity: 0.8;
}
.save{
  display: flex;
  justify-content: center;
  flex-grow: 1.0;
  padding: 8px 8px;
  background-color: #DEA681;
  color: white;
  border-radius: 4px;
}
.save_hover{
  opacity: 0.8;
}